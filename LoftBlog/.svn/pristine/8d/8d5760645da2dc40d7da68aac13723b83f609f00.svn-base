package nasledovanie;

public class Developer extends Person
{

	Developer(String firstName, String lastName, int id) //класс наследник говорит о том, что сть конструктор с параметрами
	{
		super(firstName, lastName, id);
		
	}
	
	// методы класса
	protected void develop(String task) //Особенная характеритика класса - Девелопер 
	{
		System.out.println("I/'m doing task: " + task + " now");
	}
	
	@Override
	protected void changeId()
	{
		int devId = getId()+2; 
		
	}
	
	
	
}
