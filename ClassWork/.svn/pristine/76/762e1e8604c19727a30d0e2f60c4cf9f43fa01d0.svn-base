public class TextChar {
	public static void main(String[] args) {
		System.out.println("Вывод на консоль всех символов англ клавиатуры");
		//ВЫВОД ВСЕГО ДИАПАЗОНА СИМВОЛОВ ACII
		for (char i=32; i<=127; i++) {
			System.out.print(i + " ");
		}
		System.out.println();
		System.out.println();
		System.out.println("Вывод на консоль АНГЛ. алфавита");
		//Вывод на консоль АНГЛ. алфавита 
		for (char c='A'; c<='Z'; c++) {
			System.out.print(c + " ");
		}
		System.out.println();
		System.out.println();
		System.out.println("Вывод на консоль АНГЛ. мал. алфавита");
		//Вывод на консоль АНГЛ. мал. алфавита 
		for (char c='a'; c<='z'; c++) {
			System.out.print(c + " ");
		}
		System.out.println();
		System.out.println();
		System.out.println("Вывод на консоль РУС. алфавита");
		//Вывод на консоль РУС. алфавита
		for (char d='А'; d<='Я'; d++) {
			System.out.print(d + " ");
		}
		System.out.println();
		System.out.println();
		System.out.println("Вывод на консоль РУС. мал алфавита");
		//Вывод на консоль РУС. алфавита
		for (char d='а'; d<='я'; d++) {
			System.out.print(d + " ");
		}
		System.out.println();
		System.out.println();
		System.out.println();

	}
}
