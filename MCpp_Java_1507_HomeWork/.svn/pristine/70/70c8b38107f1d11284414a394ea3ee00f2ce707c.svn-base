package arrayList;

import java.util.ArrayList;

public class Task01 {

	public static void main(String[] args) 
	{

		ArrayList<String> al = new ArrayList<String> ();
		// add - добавляет в коллекцию данную
		al.add("Привет");
		al.add("Пока");
		System.out.println(al.size());
		
		for (int i=0; i<al.size(); i++)
		{
			System.out.println(al.get(i));
		}
		System.out.println();
		
		// в сзданные ячейки вносится корретировки
		al.set(0, "НахерПОхер");
		al.set(1, "ПЗДЦ_ПОхер");
		
		for (int i=0; i<al.size(); i++)
		{
			System.out.println(al.get(i));
		}
		
		//удаление ячеек
		
		al.remove(0);
	}

}
