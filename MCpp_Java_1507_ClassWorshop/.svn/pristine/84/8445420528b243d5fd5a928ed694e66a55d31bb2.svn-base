package lesson1;

public class Nums {

	
	public static void main(String[] args) {

		int a=5;
		int b=10;
		int c=-8;
		int sum=0;
		
		if (a>0) {
			sum = sum + a;
		}
		
		if (b>0) {
			sum = sum + b;
					
		}
		
		if (c>0) {
			sum = sum + c;
		}
		
		System.out.println("Сумма только положительных из трех чисел: " + sum);
	}
}
