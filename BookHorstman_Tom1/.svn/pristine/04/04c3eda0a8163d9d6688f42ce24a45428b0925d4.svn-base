package Glava_03;

public class Task01 {

	public static void main(String[] args) 
	{
		int a=7;
		int b=7;
		
		int c = 2 * ++a;
		int d = 2 * b++;
			
		System.out.println(c);
		System.out.println(d);
		
		System.out.println();
		
		
		String bbb = "Ебанный в рот";
		String lowerCase = bbb.toLowerCase();
		String trim = bbb.trim();
		System.out.println("Колличество букв в слове - " + bbb.length());
		
		System.out.println(bbb);
		System.out.println(lowerCase);
		System.out.println(trim);
	}

}
