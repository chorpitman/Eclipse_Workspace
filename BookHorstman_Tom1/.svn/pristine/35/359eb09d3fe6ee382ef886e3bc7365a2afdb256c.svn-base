package Test1;

import java.util.Date;

public class Car 
{
	// Конструктор класса Car
	public Car (String aBrand, String bModel, String cEquipment,  
			String dColor, int eYearOfProduct, double fPrice)
	{
		this.brand = aBrand;
		this.model = bModel;
		this.equipment = cEquipment;
		this.color = dColor;
		this.yearOfProduct = eYearOfProduct;
		this.price = fPrice;	
	}

	// Методы работы над классом
	public String getBrand()
	{
		return brand;
	}
	
	public String getModel()
	{
		return model;
	}
	
	public String getEquipment()
	{
		return equipment;
	}
	
	public String getColor()
	{
		return color;
	}
	
	public int getYearOfProduct()
	{
		return yearOfProduct;
	}
	
	public double getPrice()
	{
		return price;
	}

	//Переменный для накопления результата
	private String brand;
	private String model;
	private String equipment;
	private String color; 
	private int yearOfProduct;
	private double price;
}
