package Glava_03;

import javax.swing.JOptionPane;

public class Input_Output_Swing {

	public static void main(String[] args)
	{
		String firstName = JOptionPane.showInputDialog("Как Вас зовут?");
		String input = JOptionPane.showInputDialog("Сколько Вам лет?");
		int age = Integer.parseInt(input);
		
		System.out.println(firstName + ", в следующем году Вам будет " + (age+1));
	}

}
